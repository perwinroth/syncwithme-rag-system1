{
  "name": "syncwithme-rag-system",
  "version": "2.0.0",
  "description": "Global RAG system for travel intelligence with automated deployment",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:unified": "tsx watch src/server-unified.ts",
    "dev:discovery": "tsx watch src/server-discovery.ts",
    "dev:global": "tsx watch src/server-global.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "deploy": "./scripts/quick-deploy.sh",
    "deploy:message": "./scripts/quick-deploy.sh",
    "sync-corpuses": "tsx src/scripts/sync-corpuses.ts",
    "process-corpuses": "tsx src/scripts/process-corpuses.ts",
    "docker:build": "docker build -t syncwithme-rag .",
    "docker:run": "docker run -p 3001:3001 --env-file .env syncwithme-rag",
    "generate-corpus": "tsx src/scripts/generate-corpus.ts",
    "generate-test-corpus": "tsx src/scripts/generate-test-corpus.ts",
    "create-global-corpus": "tsx src/scripts/create-global-corpus.ts",
    "create-massive-corpus": "tsx src/scripts/create-massive-corpus.ts",
    "train-global-corpus": "tsx src/scripts/train-with-global-corpus.ts",
    "train-massive-corpus": "tsx src/scripts/train-with-global-corpus.ts --massive",
    "quick-upload": "tsx src/scripts/quick-pinecone-upload.ts",
    "fixed-upload": "tsx src/scripts/fixed-pinecone-upload.ts",
    "snapshot": "node scripts/snapshot.js"
  },
  "dependencies": {
    "@langchain/openai": "^0.0.14",
    "@langchain/pinecone": "^0.0.4",
    "@pinecone-database/pinecone": "^2.0.0",
    "@types/cheerio": "^0.22.35",
    "@types/html2canvas": "^0.5.35",
    "@types/node-fetch": "^2.6.13",
    "@types/socket.io": "^3.0.1",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@vercel/blob": "^0.19.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "cron": "^4.3.3",
    "dotenv": "^16.4.0",
    "express": "^4.21.2",
    "helmet": "^7.1.0",
    "html2canvas": "^1.4.1",
    "ical-generator": "^9.0.0",
    "jspdf": "^3.0.1",
    "langchain": "^0.1.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.28.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "ws": "^8.18.3",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "playwright": "^1.47.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "rag",
    "travel",
    "ai",
    "pinecone",
    "openai",
    "railway",
    "automation",
    "global"
  ],
  "author": "Per Winroth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/perwinroth/syncwithme-rag-system1.git"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
