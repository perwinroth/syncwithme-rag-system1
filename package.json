    "generate-corpus": "tsx src/scripts/generate-corpus.ts"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^2.0.0",
    "@vercel/blob": "^0.19.0",
    "openai": "^4.28.0",
    "langchain": "^0.1.0",
    "@langchain/openai": "^0.0.14",
    "@langchain/pinecone": "^0.0.4",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.4.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0"
  }
}