{
  "name": "syncwithme-rag-system",
  "version": "1.0.0",
  "description": "Dual RAG system for travel intelligence",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "deploy": "vercel",
    "sync-corpuses": "tsx src/scripts/sync-corpuses.ts",
    "process-corpuses": "tsx src/scripts/process-corpuses.ts",
    "docker:build": "docker build -t syncwithme-rag .",
